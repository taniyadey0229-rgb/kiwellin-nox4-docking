table.tocArticle td.tocGalleys { display: block; position: relative; text-align: center; padding: 0.5em; right: 0px; float: right; background: rgb(253, 254, 222); width: 110px; border-width: 1px; border-style: solid; border-color: rgb(222, 214, 168); }

table.tocArticle td.tocPages { display: block; float: right; position: relative; text-align: center; width: 110px; padding: 0px 0.5em; }

div.separator { border-bottom: 1px dotted rgb(164, 33, 49); padding: 1em 0px; }

table ~ div.separator { border-bottom: none; }

#articleCoverImage img { margin: 0px auto; padding: 2em; }

#content #articleAbstract h4, #content #articleSubject h4, #content #articleCitations h4, #content #articleFullText h4 { border-bottom: 1px dotted rgb(164, 33, 49); padding-bottom: 0.75em; padding-top: 1em; }

div#issue { margin-top: 3em; clear: both !important; }

h4.issue-details { border-bottom: 1px dotted rgb(164, 33, 49); width: 62%; padding-top: 1em !important; padding-bottom: 0.5em !important; margin-bottom: 0.75em !important; }

p.issue-details { margin-bottom: 0px; }

p.issue-details span { font-style: italic; }

p.issue-description img:first-child { height: 100px; float: left; margin: 0.5em 1em 1em 0px; }

